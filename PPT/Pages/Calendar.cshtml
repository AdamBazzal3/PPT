@page
@model PPT.Pages.CalendarModel
@{
    ViewData["Title"] = "المفكرة";
}

<div class="wrapper" >

    @Html.AntiForgeryToken()
        <div id="demo" class="alert alert-danger" role="alert" style="display:none;">
        </div>
        <header>
            <p class="current-date"></p>
            <div class="icons">
            <span id="prev" class="material-symbols-rounded">&#8249;</span>
            <span id="next" class="material-symbols-rounded">&#8250;</span>
            
            </div>
        </header>
        <div class="calendar">
            <ul class="weeks">
                <li>Sun</li>
                <li>Mon</li>
                <li>Tues</li>
                <li>Wed</li>
                <li>Thurs</li>
                <li>Fri</li>
                <li>Sat</li>
            </ul>
            <ul class="days"></ul>
        </div>
        
    </div>
<script>
    window.onload = function () {
        renderCalendar();
    }

</script>
