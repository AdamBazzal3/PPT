@page
@model PPT.Pages.ImportdataModel
@{
    ViewData["Title"] = "ImportData";
}
<h1>تحميل الملف</h1>

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="UploadFile">يرجى إختيار الملف بصيغة CSV</label>
        <input type="file" id="UploadFile" name="UploadFile" class="form-control-file" />
    </div>
    <button type="submit" class="btn btn-primary">تحميل</button>
</form>
@if (!string.IsNullOrEmpty(Model.FileContent))
{
    <h3>File Content:</h3>
    <pre>@Model.FileContent</pre>
}